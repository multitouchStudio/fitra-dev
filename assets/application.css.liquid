body {
  background-color: #FF3700;
}

.header-nav {
    display: flex;
    justify-content: space-between;
    margin: 0;
    padding: 1px;
    background-color: gray;
}


.middle-nav-links {
    display: flex;
    font-size: 17px;
    font-weight: bold;
}

.middle-nav-links li {
    padding: 10px;
    list-style-type: none;
}

.middle-nav-links li:hover {
    text-decoration: underline;
}

.middle-nav-links li a {
    text-decoration: none;
    color: inherit;
}
